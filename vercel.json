{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "functions": {
    "api/status.js": {
      "runtime": "nodejs18.x"
    },
    "api/hello.js": {
      "runtime": "nodejs18.x"
    },
    "api/test/index.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
